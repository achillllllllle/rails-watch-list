<div class="d-flex justify-content-between bg-secondary bg-gradient p-3">
  <div>
    <h1 class="text-white fw-bold fs-1">Lists</h1>
  </div>
  <div>
    <%= link_to "New list", new_list_path, class:"text-white fw-bold fs-1" %>
  </div>
</div>
<div>
  <% @lists.each do |list| %>
    <div>
      <%= link_to "#{list.name}", list_path(list.id) %>
    </div>
  <% end %>
</div>
